#!/bin/bash
[ -x "$(command -v direnv)" ] || (curl -sfL https://direnv.net/install.sh | bash)
[ -x "$(command -v starship)" ] || (curl -sS https://starship.rs/install.sh | sh)
[ -d "$HOME/.asdf" ] || git clone https://github.com/asdf-vm/asdf.git ~/.asdf --branch v0.9.0


if [ -n "$ZSH_VERSION" ]; then
	echo -n ""
fi

if [ -n "$BASH_VERSION" ]; then
	echo -n ""
fi
